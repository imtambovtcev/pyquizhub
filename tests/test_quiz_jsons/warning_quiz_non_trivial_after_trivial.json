{
    "metadata": {
        "title": "Warning Non-Trivial After Trivial",
        "description": "Tests for warnings when a non-trivial condition follows a trivial one",
        "author": "Test Author",
        "version": "2.0"
    },
    "variables": {
        "score_a": {
            "type": "integer",
            "mutable_by": ["engine"],
            "tags": ["score", "public"]
        }
    },
    "questions": [
        {
            "id": 1,
            "data": {
                "text": "Do you like apples?",
                "type": "multiple_choice",
                "options": [
                    {
                        "value": "yes",
                        "label": "Yes"
                    },
                    {
                        "value": "no",
                        "label": "No"
                    }
                ]
            },
            "score_updates": []
        }
    ],
    "transitions": {
        "1": [
            {
                "expression": "true",
                "next_question_id": 1
            },
            {
                "expression": "score_a > 0",
                "next_question_id": 1
            }
        ]
    }
}
