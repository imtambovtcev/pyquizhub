        {
            "id": "weather_api",
            "method": "GET",
            "prepare_request": {
                "body_template": "{\"city\": \"{city}\"}"
            },
            "extract_response": {
                "temperature": "data.main.temp"
            }
        }
    ],
    "questions": [
        {
            "id": 1,
            "data": {
                "text": "Enter your city",
                "type": "text"
            },
            "score_updates": []
        }
    ],
    "transitions": {
        "1": [
            {"expression": "true", "next_question_id": null}
        ]
    }
